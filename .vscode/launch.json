{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "나무코드 실행",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceRoot}"],
      "outFiles": [
        "${workspaceRoot}/dist/**/*.js"
      ],
      "autoAttachChildProcesses": true
    }
  ]
}
